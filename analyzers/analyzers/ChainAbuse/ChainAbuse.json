{
  "name": "ChainAbuse",
  "version": "1.1",
  "author": "Peter Juhas; Fabien Bloume, StrangeBee",
  "url": "https://github.com/pjuhas/Cortex-Analyzers",
  "license": "AGPL-V3",
  "description": "Check crypto address against reported abuse cases on ChainAbuse",
  "dataTypeList": [
    "crypto_address",
    "btc_address"
  ],
  "baseConfig": "ChainAbuse",
